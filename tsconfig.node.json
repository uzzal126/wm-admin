{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": false,
    "noImplicitUseStrict": false
  },
  "include": [
    "vite.config.ts",
    "utilities.ts"
  ],
  "exclude": [
    "plugins/**/*",
    "typings/**/*",
    "dist/**/*"
  ]
}
